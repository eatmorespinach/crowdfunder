<%= form_for [:my, @project] do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title%>

	<%= f.label :teaser %>
	<%= f.text_field :teaser%>

	<%= f.label :description %>
	<%= f.text_field :description%>

	<%= f.label :goal %>
	<%= f.password_field :goal %>

	<%= f.submit "Update Project" %>
<% end %>


<%= link_to "Manage Images", my_project_images_path(@project) %> 