<%= form_for [:my, @project], :html => {:multipart => true} do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title%>

	<%= f.label :teaser %>
	<%= f.text_field :teaser%>

	<%= f.label :description %>
	<%= f.text_field :description%>

	<%= f.label :goal %>
	<%= f.text_field :goal %>

	<%= f.label :image %>
	<%= f.file_field :image %>

	<%= f.submit "Publish Project" %>
<% end %>